{
  "name": "nodejs-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node index.js",
    "stage:setup": "mup setup --config=./deploy/mup.staging.js",
    "stage:deploy": "npm run stage:build && mup deploy --config=./deploy/mup.staging.js",
    "stage:reconfig": "mup reconfig --config=./deploy/mup.staging.js",
    "stage:logs": "mup logs --config=./deploy/mup.staging.js --tail 500 -f",
    "stage:build": "rm -rf dist && webpack --config webpack.babel.js --progress",
    "stage:start": "node ./dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.29.0",
    "@babel/node": "^7.29.0",
    "@babel/plugin-proposal-export-default-from": "^7.27.1",
    "@babel/preset-env": "^7.29.0",
    "axios": "^1.13.5",
    "babel-loader": "^10.0.0",
    "bcryptjs": "^3.0.3",
    "core-js": "^3.48.0",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "lodash": "^4.17.23",
    "morgan": "^1.10.1",
    "mup": "^1.5.11",
    "mup-node": "^0.8.2",
    "nodemailer": "^8.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.19.0",
    "pg-hstore": "^2.3.4",
    "regenerator-runtime": "^0.14.1",
    "sequelize": "^6.37.7",
    "webpack": "^5.105.2",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.6",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "nodemon": "^3.1.14",
    "prettier": "^3.8.1"
  }
}
